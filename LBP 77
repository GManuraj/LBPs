#include<stdio.h>
int main()
{
    int n,count=0,digit,count1=0;
    scanf("%d",&n);
    while(n>0)
    {
        digit=n%10;
        if(digit==1)
        {
            count++;
        }
        if(digit==0)
        {
            count1++;
        }
        n=n/10;
    }
    printf("%d %d",count,count1);
}
