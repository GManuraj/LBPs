#include <stdio.h>
int isPrime(int num) {
    int i,count=0;
    for(i=1;i<=num;i++){
        if(num%i==0){
            count++;
        }
    }
    if(count==2){
        return 1;
    }
    return 0;
}

int sumPrimes(int start, int end) {
    int sum = 0;
    for(int i = start; i <= end; i++) {
        if(isPrime(i))
            sum += i;
    }
    return sum;
}
int main() {
    int start, end;
    scanf("%d", &start);
    scanf("%d", &end);
    printf("%d", sumPrimes(start, end));
}
