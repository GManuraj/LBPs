#include<stdio.h>
int main()
{
    int num,mul=1,digit,res,sum=0,temp;
    scanf("%d",&num);
    temp=num;
    if(num>=10 && num<=99){
        while(temp!=0)
        {
            digit=temp%10;
            sum=sum+digit;
            mul=digit*mul;
            temp=temp/10;
        }
        res=sum+mul;
        if(num==res){
            printf("YES");
        }
        else{
            printf("NO");
        }
    }
    else{
        printf("PLEASE ENTER 2-DIGIT NUMBER");
    }
}
