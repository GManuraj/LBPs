#include <stdio.h>
int prime(int n);
int main()
{
    int strt,end,i,sum=0,count=0;
    float average;
    scanf("%d %d",&strt,&end);
    for(i=strt;i<=end;i++)
    {
        if(prime(i))
        {
            printf("%d\t",i);
            sum+=i;
            count++;
        }
    }
    if(count>0)
    {
        average=sum/count;
        printf("\n%.2f",average);
    }
    
}
int prime(int n)
{
    int count=0;
        for(int j=1;j<=n;j++)
        {
            if(n%j==0)
            {
                count++;
            }
        }
        if(count==2)
        {
            return 1;
        }
        else
        {
            return 0;
        }
        
}
