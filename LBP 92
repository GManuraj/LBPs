#include<stdio.h>
int BlackJack(int n1,int n2);
int main() 
{
   int n1,n2;
   scanf("%d %d",&n1,&n2);
   BlackJack(n1,n2);
}
int BlackJack(int n1,int n2){
    if(n1<=21 && n2<=21)
    {
        if(n1<n2){
            printf("%d",n2);
            
        }
        if(n1>n2){
            printf("%d",n1);
        }
    }
    else{
        if(n1<21 && n2>21){
            printf("%d",n1);
        }
        if(n1>21 && n2<21){
            printf("%d",n2);
        }
    }
}
