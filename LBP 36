#include<stdio.h>
void fib(int n);
int main()
{
    int n;
    scanf("%d",&n);
    fib(n);
}
void fib(int n)
{
    int a=0,b=1,c,i;
    if(n>=0)
    {
        printf("%d\t",a);
    }
    if(n>=1)
    {
        printf("%d\t",b);
    }
    for(int i=2;i<n;i++)
    {
        c=a+b;
        printf("%d\t",c);
        a=b;
        b=c;
    }
}
