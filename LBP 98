#include <stdio.h>
int main()
{
    int n,product=1,sum=0,digit,total;
    scanf("%d",&n);
    if(n==0){
        product=0;
        sum=0;
    }
    else{
        while(n!=0){
            digit=n%10;
            product=product*digit;
            sum=sum+digit;
            n=n/10;
        }
    }
    total=product-sum;
    printf("%d",total);
}
