#include<stdio.h>
int main()
{
    int digit,number,count=0,remainder;
    scanf("%d",&number);
    scanf("%d",&digit);
    while(number!=0)
    {
        remainder=number%10;
        if(remainder==digit)
        {
            count++;
        }
        number=number/10;
    }
    printf("%d",count);
    
}
