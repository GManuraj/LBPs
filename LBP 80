#include <stdio.h>
int isPrime(int num) {
    int i,count=0;
    for(i=1;i<=num;i++)
    {
        if(num%i==0){
            count++;
        }
    }
    if(count==2)
    {
        return 1;
    }
    return 0;
}
int main() {
    int num, sum = 0;
    scanf("%d", &num);
    for (int i = 2; i <= num; i++) {
        if (num % i == 0 && isPrime(i)) {
            sum += i;
        }
    }
    printf( "%d\n",sum);
}
