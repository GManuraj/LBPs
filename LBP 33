#include<stdio.h>
#include<string.h>
int main()
{
    int i,count=0;
    char str[20];
    gets(str);
    for(i=0;i<strlen(str);i++)
    {
        
        if(str[i]=='a'|| str[i]=='e'|| str[i]=='i'|| str[i]=='o'|| str[i]=='u'|| str[i]=='A'|| str[i]=='E'|| str[i]=='I'|| str[i]=='O'|| str[i]=='U')
        {
            count++;
        }
    }
    printf("%d",count);
}
