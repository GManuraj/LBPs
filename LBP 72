#include<stdio.h>
int prime(int n);
int main()
{
    int n,sum=0,digit;
    scanf("%d",&n);
    while(n>0)
    {
        digit=n%10;
        if(prime(digit))
        {
            sum+=digit;
        }
        n/=10;
    }
    printf("%d",sum);
}
int prime(int n){
    int i,count=0;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            count++;
        }
    }
    if(count==2)
    {
        return 1;
    }
    else
    {
        return 0;
    }
}
